<h1>Sign in</h1>

<form action="<%= session_url %>" method="post">
    <label> Username:
    <input type="text" name="user[user_name]" value="<%= @user.user_name %>" >
    </label>
    <br>
    <br>

    <label> Password:
    <input type="password" name="user[password]" value="<%= @user.password %>" >
    </label>
    <br>
    <br>

    <input type="submit" value="Login">

</form>